def bubble_sort(arr: List[T], key: Callable[[T], Any] = None, reverse: bool = False) -> List[T]:
    """
    Sort an array using the bubble sort algorithm.

    Bubble sort repeatedly steps through the list, compares adjacent elements
    and swaps them if they're in the wrong order. Time complexity: O(n¬≤).

    Args:
        arr: List of comparable items to sort
        key: Optional function to extract comparison key from each element
        reverse: If True, sort in descending order

    Returns:
        A new sorted list (original list remains unchanged)

    Examples:
        >>> bubble_sort([64, 34, 25, 12, 22, 11, 90])
        [11, 12, 22, 25, 34, 64, 90]

        >>> bubble_sort(['apple', 'banana', 'cherry'], reverse=True)
        ['cherry', 'banana', 'apple']

        >>> bubble_sort([{'name': 'Bob', 'age': 25}, {'name': 'Alice', 'age': 30}],
                      key=lambda x: x['age'])
        [{'name': 'Bob', 'age': 25}, {'name': 'Alice', 'age': 30}]
    """
    # Make a copy to avoid modifying the original list
    sorted_arr = arr.copy()
    n = len(sorted_arr)

    # Use identity function if no key is provided
    if key is None:
        key = lambda x: x

    for i in range(n):
        # Track if any swaps were made for optimization
        swapped = False

        for j in range(0, n - i - 1):
            # Compare adjacent elements
            comparison = key(sorted_arr[j]) > key(sorted_arr[j + 1])
            if reverse:
                comparison = key(sorted_arr[j]) < key(sorted_arr[j + 1])

            if comparison:
                # Swap elements
                sorted_arr[j], sorted_arr[j + 1] = sorted_arr[j + 1], sorted_arr[j]
                swapped = True

        # If no swaps were made, the list is sorted
        if not swapped:
            break

    return sorted_arr


