{
  "name": "stigmergy",
  "version": "1.0.70",
  "type": "commonjs",
  "description": "Stigmergy CLI - Multi-Agents Cross-AI CLI Tools Collaboration System",
  "main": "src/main.js",
  "bin": {
    "stigmergy": "src/main.js"
  },
  "scripts": {
    "start": "node src/main.js",
    "test": "node test/real-test.js",
    "test:all": "node test/test-all-clis.js",
    "test:cross-cli": "node test/cross-cli-real-test.js",
    "build": "node scripts/build.js",
    "deploy": "node src/deploy.js",
    "status": "node src/index.js status",
    "scan": "node src/index.js scan",
    "init": "node src/index.js init",
    "validate": "node src/index.js validate",
    "clean": "node src/index.js clean",
    "dev": "node --watch src/index.js",
    "lint": "eslint src/",
    "format": "prettier --write src/"
  },
  "keywords": [
    "ai",
    "cli",
    "collaboration",
    "multi-agents",
    "nodejs-first",
    "ansi-encoding",
    "cross-cli",
    "claude-cli",
    "gemini-cli",
    "qwen-cli",
    "iflow-cli",
    "real-testing",
    "python-free"
  ],
  "files": [
    "src/**/*.js",
    "config/**/*.json",
    "templates/**/*.md",
    "bin/**/*.cmd",
    "test/**/*.js",
    "scripts/**/*.js"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ptreezh/stigmergy-CLI-Multi-Agents.git"
  },
  "author": "Stigmergy CLI Team",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "commander": "^12.0.0",
    "inquirer": "^8.2.6",
    "chalk": "^4.1.2",
    "js-yaml": "^4.1.0",
    "node-fetch": "^2.6.7",
    "child_process": "^1.0.2",
    "fs-extra": "^11.1.1",
    "path": "^0.12.7",
    "os": "^0.1.2",
    "crypto": "^1.0.1",
    "events": "^3.3.0",
    "util": "^0.12.5",
    "semver": "^7.5.4",
    "glob": "^10.3.10",
    "chokidar": "^3.5.3",
    "table": "^6.8.1",
    "figures": "^3.2.0",
    "ora": "^5.4.1"
  },
  "devDependencies": {
    "eslint": "^8.50.0",
    "prettier": "^3.0.3",
    "nodemon": "^3.0.1"
  },
  "config": {
    "encoding": "ansi",
    "platform": "nodejs-first",
    "python-free": true,
    "real-testing": true,
    "unicode-free": true
  },
  "npx": {
    "stigmergy": "src/main.js"
  },
  "homepage": "https://github.com/ptreezh/stigmergy-CLI-Multi-Agents#readme",
  "bugs": {
    "url": "https://github.com/ptreezh/stigmergy-CLI-Multi-Agents/issues"
  }
}
