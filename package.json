{
  "name": "stigmergy",
  "version": "1.2.0",
  "description": "Stigmergy CLI - Multi-Agents Cross-AI CLI Tools Collaboration System",
  "main": "src/index.js",
  "bin": {
    "stigmergy": "bin/stigmergy"
  },
  "scripts": {
    "start": "node src/index.js",
    "test": "jest",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration",
    "test:e2e": "jest tests/e2e",
    "coverage": "jest --coverage",
    "build": "node scripts/build.js",
    "deploy": "node src/index.js deploy",
    "status": "node src/index.js status",
    "scan": "node src/index.js scan",
    "init": "node src/index.js init",
    "validate": "node src/index.js validate",
    "dev": "node --watch src/index.js",
    "lint": "eslint src/",
    "format": "prettier --write src/",
    "clean": "node src/index.js clean",
    "diagnostic": "node src/index.js diagnostic",
    "uninstall": "node -e \"const EU=require('./src/core/enhanced_uninstaller'); new EU({dryRun:true,verbose:true}).completeUninstall()\"",
    "uninstall-force": "node -e \"const EU=require('./src/core/enhanced_uninstaller'); new EU({force:true,verbose:true}).completeUninstall()\"",
    "post-deployment-config": "node scripts/post-deployment-config.js",
    "preinstall": "node scripts/preinstall-check.js",
    "postinstall": "node src/index.js auto-install"
  },
  "keywords": [
    "ai",
    "cli",
    "collaboration",
    "multi-agents",
    "nodejs-first",
    "ansi-encoding",
    "cross-cli",
    "claude-cli",
    "gemini-cli",
    "qwen-cli",
    "iflow-cli",
    "real-testing",
    "python-free"
  ],
  "files": [
    "src/**/*.js",
    "config/**/*.json",
    "templates/**/*.md",
    "bin/**/*",
    "test/**/*.js",
    "scripts/**/*.js",
    "examples/**/*",
    "docs/**/*",
    "package.json",
    "README.md",
    "LICENSE",
    "STIGMERGY.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ptreezh/stigmergy-CLI-Multi-Agents.git"
  },
  "author": "Stigmergy CLI Team",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "chokidar": "^3.5.3",
    "commander": "^9.5.0",
    "figures": "^3.2.0",
    "fs-extra": "^11.2.0",
    "glob": "^8.1.0",
    "inquirer": "^8.2.6",
    "js-yaml": "^4.1.0",
    "ora": "^5.4.1",
    "semver": "^7.6.3",
    "table": "^6.8.2"
  },
  "devDependencies": {
    "eslint": "^8.57.1",
    "jest": "^29.7.0",
    "nodemon": "^3.1.7",
    "prettier": "^3.3.3"
  },
  "config": {
    "encoding": "ansi",
    "platform": "nodejs-first",
    "python-free": true,
    "real-testing": true,
    "unicode-free": true
  },
  "npx": {
    "stigmergy": "src/index.js"
  },
  "homepage": "https://github.com/ptreezh/stigmergy-CLI-Multi-Agents#readme",
  "bugs": {
    "url": "https://github.com/ptreezh/stigmergy-CLI-Multi-Agents/issues"
  }
}
