{
  "name": "@stigmergy/sessions",
  "version": "1.0.0",
  "description": "Cross-CLI session history management and recovery system",
  "main": "dist/lib.js",
  "bin": {
    "sg-sessions": "dist/cli.js",
    "cross-cli-history": "dist/cli.js"
  },
  "types": "dist/lib.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --testPathPattern=integration",
    "test:e2e": "jest --testPathPattern=e2e",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "dev": "ts-node src/cli.ts",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run clean && npm run build && npm test"
  },
  "keywords": [
    "cli",
    "ai",
    "session",
    "history",
    "recovery",
    "claude",
    "gemini",
    "qwen",
    "iflow"
