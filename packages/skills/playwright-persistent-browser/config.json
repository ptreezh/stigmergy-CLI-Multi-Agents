{
  "session": {
    "name": "my-playwright-session",
    "path": "./my-playwright-session"
  },
  "browser": {
    "headless": false,
    "slowMo": 100,
    "viewport": {
      "width": 1280,
      "height": 720
    },
    "args": [
      "--no-sandbox",
      "--disable-blink-features=AutomationControlled",
      "--disable-features=IsolateOrigins,site-per-process"
    ]
  },
  "timeouts": {
    "default": 30000,
    "loginCheck": 5000,
    "navigation": 10000
  },
  "sites": {
    "github": {
      "url": "https://github.com",
      "loginSelector": "button[aria-label=\"View profile and more\"]",
      "logoutSelector": "button[aria-label=\"Sign out\"]"
    },
    "google": {
      "url": "https://accounts.google.com",
      "loginSelector": "[data-email]",
      "logoutSelector": "[aria-label=\"Google account\"]"
    }
  }
}