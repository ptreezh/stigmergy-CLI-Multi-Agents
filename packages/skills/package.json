{
  "name": "@stigmergy/skills",
  "version": "1.0.0",
  "type": "module",
  "description": "Stigmergy AI Skills System - Natural Language Skills for AI CLI Tools",
  "main": "src/index.js",
  "bin": {
    "sg-skills": "src/index.js",
    "stigmergy-skills": "src/index.js"
  },
  "scripts": {
    "start": "node src/index.js",
    "test": "node test/all-tests.js",
    "test:unit": "node test/unit/*.test.cjs",
    "test:integration": "node test/integration/*.test.cjs",
    "test:e2e": "node test/e2e/*.test.cjs",
    "test:encoding": "node test/encoding-tests.js",
    "test:natural-language": "node test/natural-language-tests.js",
    "validate": "npm run test && npm run build",
    "build": "node build/build.cjs",
    "lint": "eslint src/**/*.js test/**/*.js",
    "lint:fix": "eslint src/**/*.js test/**/*.js --fix",
    "prepare": "npm run build",
    "prepublishOnly": "npm run test && npm run lint"
  },
  "keywords": [
    "ai",
    "cli",
    "skills",
    "natural-language",
    "claude",
    "gemini",
    "qwen",
    "automation",
    "translation",
    "code-analysis",
    "code-generation",
    "documentation",
    "stigmergy"
  ],
  "author": "Stigmergy CLI Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ptreezh/stigmergy-CLI-Multi-Agents.git",
    "directory": "packages/skills"
  },
  "bugs": {
    "url": "https://github.com/ptreezh/stigmergy-CLI-Multi-Agents/issues"
  },
  "homepage": "https://github.com/ptreezh/stigmergy-CLI-Multi-Agents/tree/main/packages/skills",
  "engines": {
    "node": ">=14.0.0"
  },
  "files": [
    "src/",
    "test/",
    "build/",
    "README.md",
    "LICENSE"
  ],
  "dependencies": {
    "@telagod/papermcp": "^0.3.5",
    "aigroup-paper-mcp": "^0.3.6",
    "chalk": "^4.1.2",
    "commander": "^9.4.1",
    "inquirer": "^8.2.4",
    "ora": "^5.4.1"
  },
  "peerDependencies": {
    "stigmergy": "^1.2.1"
  },
  "peerDependenciesMeta": {
    "stigmergy": {
      "optional": true
    }
  },
  "devDependencies": {
    "eslint": "^8.0.0",
    "jest": "^29.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "stigmergy": {
    "type": "@stigmergy/skills",
    "compatibility": ">=1.2.1",
    "core-dependency": "stigmergy"
  }
}
