{
  "name": "stigmergy",
  "version": "1.3.54-beta.0",
  "description": "Stigmergy CLI - Multi-Agents Cross-AI CLI Tools Collaboration System",
  "main": "src/index.js",
  "bin": {
    "stigmergy": "bin/stigmergy"
  },
  "scripts": {
    "start": "node src/index.js",
    "build:orchestration": "tsc --project tsconfig.build.json",
    "concurrent": "node src/index.js concurrent",
    "interactive": "node src/index.js interactive",
    "status": "node src/index.js status",
    "scan": "node src/index.js scan",
    "install": "node src/index.js install",
    "init": "node src/index.js init",
    "setup": "node src/index.js setup",
    "call": "node src/index.js call",
    "clean": "node cleanup.js",
    "audit": "npm audit --audit-level=moderate",
    "audit:fix": "npm audit fix",
    "check-updates": "npm outdated",
    "analyze-deps": "depcheck",
    "check-unused": "npm-check"
  },
  "keywords": [
    "ai",
    "cli",
    "collaboration",
    "multi-agents",
    "nodejs-first",
    "cross-cli",
    "claude-cli",
    "gemini-cli",
    "qwen-cli",
    "iflow-cli",
    "real-testing",
    "python-free"
  ],
  "files": [
    "src/**/*.js",
    "config/**/*.json",
    "bin/**/*",
    "package.json",
    "README.md",
    "LICENSE",
    "STIGMERGY.md"
  ],
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "commander": "^11.1.0",
    "inquirer": "^8.2.6",
    "js-yaml": "^4.1.1",
    "semver": "^7.7.3"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^20.14.0",
    "depcheck": "^1.4.7",
    "eslint": "^8.57.1",
    "jest": "^29.7.0",
    "npm-check": "^6.0.4",
    "prettier": "^3.3.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3"
  },
  "optionalDependencies": {
    "fs-extra": "^11.3.3",
    "rimraf": "^5.0.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ptreezh/stigmergy-CLI-Multi-Agents.git"
  },
  "author": "Stigmergy CLI Team",
  "contributors": [
    "Claude",
    "Qwen",
    "iFlow",
    "QoderCLI",
    "GLM4.5",
    "Other AI Systems"
  ],
  "license": "MIT"
}
